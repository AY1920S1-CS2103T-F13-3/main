@startuml
!include style.puml

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":MoneyGoWhereParser" as MoneyGoWhereParser LOGIC_COLOR
participant ":AddReminderCommandParser" as AddReminderCommandParser LOGIC_COLOR
participant "d:AddReminderCommand" as AddReminderCommand LOGIC_COLOR
participant "d:Reminder" as Reminder LOGIC_COLOR
participant "d:Date" as Date LOGIC_COLOR
participant "d:ReminderMessage" as ReminderMessage LOGIC_COLOR
participant ":CommandResult" as CommandResult LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
end box

[-> LogicManager : execute("reminder d/30/08/2020 m/Pay school fee")
activate LogicManager

LogicManager -> MoneyGoWhereParser : parseCommand("reminder d/30/08/2020 m/Pay school fee")
activate MoneyGoWhereParser

create AddReminderCommandParser
MoneyGoWhereParser -> AddReminderCommandParser
activate AddReminderCommandParser

AddReminderCommandParser --> MoneyGoWhereParser
deactivate AddReminderCommandParser


@enduml
